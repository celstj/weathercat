{
  "name": "first-react-app",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://celstj.github.io/weathercat/",
  "type": "module",
  "watch": {
    "build": {
      "patterns": [
        "weathercat/src/*.js",
        "weathercat/src/modules/*.jsx",
        "weathercat/src/modules/*.js",
        "weathercat/src/*.css",
        "weathercat/src/*.html",
        "weathercat/public/*.html",
        "weathercat/*.html"
      ],
      "extensions": [
        "jsx",
        "css",
        "js"
      ],
      "quiet": true,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "watch": "npm-watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "npm-watch": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/eslint-parser": "^7.24.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "vite": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
